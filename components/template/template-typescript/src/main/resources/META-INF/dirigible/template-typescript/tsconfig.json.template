{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ES6",
        "moduleResolution": "Node",
        "baseUrl": "../",
        "lib": [
            "ESNext",
            "DOM"
        ],
        "paths": {
            "@aerokit/sdk/*": [
                "../modules/src/*"
            ],
            "sdk/*": [
