    <body class="bk-vbox">
        <bk-scrollbar class="bk-full-height">
            <bk-fieldset class="fd-padding-begin-end--sm" ng-form="forms.details">
                <bk-form-group label="{{formHeaders[action]}}" name="entityForm" transclude-classes="fd-container fd-form-layout-grid-container">
#foreach ($property in $properties)
#if(!$property.dataAutoIncrement)
#if($property.widgetType == "DROPDOWN")
                    <bk-form-item class="fd-row">
                    	<div class="fd-col fd-col-md--2">
                        	<bk-form-label for="id${property.name}" ng-required="${property.isRequiredProperty}" colon="true">{{ '$projectName:${tprefix}.t.$property.dataName' | t }}</bk-form-label>
                        </div>
                        <div class="fd-col #if($property.widgetSize)${property.widgetSize}#else fd-col-md--7 #end">
	                        <bk-combobox-input input-id="id${property.name}"
								#if($property.widgetDropDownMultiSelect)
								multi-select="true"
								#end
	                            name="${property.name}"
	#if(!$property.isCalculatedProperty)
	                            state="{{ forms.details['${property.name}'].$valid ? '' : 'error' }}"
	                            ng-required="${property.isRequiredProperty}"
	#end
	                            ng-model="entity.${property.name}"
	#if($property.isCalculatedProperty == "true")
	                            is-readonly="true"
	#else
	                            is-readonly="action === 'select'"
	#end
	                            dropdown-items="options${property.name}"
	                            placeholder="{{ '$projectName:${tprefix}.messages.inputSearch' | t:{'name':'$t($projectName:${tprefix}.t.$property.dataName)'} }}"
								btn-aria-label="{{ '$projectName:${tprefix}.aria.showHide' | t:{'name':'${property.name}'} }}" list-aria-label="{{ '$projectName:${tprefix}.aria.options' | t:{'name':'${property.name}'} }}">
	                        </bk-combobox-input>
	                    </div>
	                    <div class="fd-col fd-col--full">
							<bk-button glyph="sap-icon--add" aria-label="{{ '$projectName:${tprefix}.defaults.add' | t }}" ng-click="create${property.name}()" ng-hide="action === 'select'"></bk-button>
							<bk-button glyph="sap-icon--refresh" aria-label="{{ '$projectName:${tprefix}.defaults.refresh' | t }}" ng-click="refresh${property.name}()" ng-hide="action === 'select'"></bk-button>
	#if($property.description)
							<span class="sap-icon sap-icon--information" role="presentation" ng-click="alert('${property.description}')"></span>
	#end
						</div>
                    </bk-form-item>
#elseif($property.widgetType == "CHECKBOX")
