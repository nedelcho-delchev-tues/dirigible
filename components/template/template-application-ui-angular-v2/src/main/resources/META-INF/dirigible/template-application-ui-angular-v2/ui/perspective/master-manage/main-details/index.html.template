                        <div class="fd-col #if($property.widgetSize)${property.widgetSize}#else fd-col-md--8 #end">
	#if(!$property.isCalculatedProperty)
							<bk-form-input-message state="{{ forms.details['${property.name}'].$valid ? '' : 'error' }}" text="{{ '$projectName:${tprefix}.messages.error.pattern' | t:{'rule':'${property.inputRule}'} }}">
	#end
								<bk-input id="id${property.name}"
	                                name="${property.name}"
	#if(!$property.isCalculatedProperty)
	                                state="{{ forms.details['${property.name}'].$valid ? '' : 'error' }}"
									ng-required="${property.isRequiredProperty}"
	#end
	                                ng-model="entity.${property.name}"
	#if($property.isCalculatedProperty == "true")
	                                ng-readonly="true"
	#else
	                                ng-readonly="action === 'select'"
	#end
	#if($property.minLength && !$property.isCalculatedProperty)
									ng-minlength="${property.minLength}"
	#end
	#if($property.maxLength && !$property.isCalculatedProperty)
									ng-maxlength="${property.maxLength}"
	#end
	#if(!$property.isCalculatedProperty)
	                                input-rules="{ patterns: ['${property.inputRule}'] }"
	#end
	                                type="text"
	                                placeholder="{{ '$projectName:${tprefix}.messages.inputEnter' | t:{'name':'$t($projectName:${tprefix}.t.$property.dataName)'} }}">
	                            </bk-input>
	#if(!$property.isCalculatedProperty)
							</bk-form-input-message>
	#end
	#if($property.description)
							<span class="sap-icon sap-icon--information" role="presentation" ng-click="alert('${property.description}')"></span>
	#end
	                    </div>
                    </bk-form-item>
#end
#end
#end
                </bk-form-group>
            </bk-fieldset>
        </bk-scrollbar>

		<bk-bar bar-design="footer" compact="true" ng-show="action !== 'select'">
			<bk-bar-right>
				<bk-bar-element>
					<bk-button ng-if="action === 'create'" label="{{'$projectName:${tprefix}.defaults.create' | t}}" state="emphasized" ng-click="create()" ng-disabled="!forms.details.$valid"></bk-button>
					<bk-button ng-if="action !== 'create'" label="{{'$projectName:${tprefix}.defaults.update' | t}}" state="emphasized" ng-click="update()" ng-disabled="!forms.details.$valid"></bk-button>
				</bk-bar-element>
				<bk-bar-element>
					<bk-button label="{{ '$projectName:${tprefix}.defaults.cancel' | t }}" ng-click="cancel()"></bk-button>
				</bk-bar-element>
			</bk-bar-right>
		</bk-bar>
		<bk-bar bar-design="footer" compact="true" ng-show="action === 'select' && entityActions.length && entity.${primaryKeysString}">
			<bk-bar-right>
				<bk-bar-element ng-repeat="action in entityActions track by $index">
					<bk-button label="{{action.translation.key | t:action.translation.options:action.label}}" ng-click="triggerEntityAction(action)"></bk-button>
				</bk-bar-element>
			</bk-bar-right>
		</bk-bar>
		<theme></theme>
    </body>

</html>
